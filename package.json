{
  "name": "gcs-cache-action",
  "version": "1.0.0",
  "description": "Cache your workload to a Google Cloud Storage bucket",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "package": "npm run build && ncc build lib/main.js -m -o dist/main && ncc build lib/post.js -m -o dist/post",
    "test": "jest --passWithNoTests",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MansaGroup/gcs-cache-action.git"
  },
  "keywords": [
    "actions",
    "gcs",
    "google-cloud",
    "cache"
  ],
  "author": "Jérémy Levilain <jeremy.levilain@getmansa.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MansaGroup/gcs-cache-action/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        85
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{md,yml,html,css,scss,json}": "prettier --write"
  },
  "homepage": "https://github.com/MansaGroup/gcs-cache-action#readme",
  "devDependencies": {
    "@commitlint/cli": "17.0.1",
    "@commitlint/config-conventional": "17.0.0",
    "@types/semver": "7.3.9",
    "@types/tmp": "0.2.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vercel/ncc": "0.33.4",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-import-helpers": "1.2.1",
    "eslint-plugin-jest": "25.7.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "eslint-plugin-unused-imports": "1.1.5",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "jest-circus": "28.1.0",
    "lint-staged": "12.4.2",
    "prettier": "2.6.2",
    "typescript": "4.7.2"
  },
  "dependencies": {
    "@actions/core": "1.8.2",
    "@actions/exec": "1.1.1",
    "@actions/github": "5.0.3",
    "@actions/glob": "0.3.0",
    "@google-cloud/storage": "6.0.1",
    "semver": "7.3.7",
    "tmp-promise": "3.0.3"
  }
}
